<h1>Sign Up</h1>
<%= form_with(model: @user, class: "col s12") do |form| %>
<div class="input-field col s12">
  <input id="email" type="email" name="user[email]" class="validate">
  <label for="email">Email</label>
</div>
<div class="input-field col s12">
  <input id="password" type="password" name="user[password]" class="validate">
  <label for="password">Password</label>
</div>
<button class="btn waves-effect waves-light" action="/users" method="post">SignUp</button>
<% end %>